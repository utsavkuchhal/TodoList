<%- include ("header")-%>
	<div class="box" id="heading">
	<h1><%= kindofday %></h1>
	</div>
	<div class="box">
		<% for(var i = 0; i < newlistitem.length; i++){%>
		<form action = "/delete" method = "post">
		<div class = "item">
		<input type="checkbox" value = <%=newlistitem[i]._id%> onChange = "this.form.submit()" name = "checkbox"> 
		<p><%=newlistitem[i].name%></p>
		</div>
		<input type = "hidden" value =<%= kindofday %> name = "listname">
		</form>
		<% }%>
		<form action="/" method="POST">
		<input type="text" name="todo" placeholder="Add to list" autocomplete = "off">
		<button type="submit" name = "button" value = <%= kindofday %>>+</button>
	</form>
	</div>

<%- include ("footer")-%>